{
  "name": "svelte-to-tsx",
  "author": "mizchi",
  "license": "MIT",
  "version": "0.0.1",
  "description": "svelte template to react component converter",
  "type": "module",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prepublishOnly": "pnpm build",
    "build": "tsc -p .",
    "test": "vitest --run",
    "typecheck": "tsc -p . --noEmit",
    "format": "rome format --write src package.json"
  },
  "keywords": ["svelte", "react", "compiler"],
  "peerDependencies": {
    "svelte": "4.*",
    "typescript": "*"
  },
  "devDependencies": {
    "@linaria/core": "^4.2.10",
    "@linaria/react": "^4.3.8",
    "@types/estree": "^1.0.1",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.14",
    "astring": "^1.8.6",
    "estree-walker": "^3.0.3",
    "magic-string": "^0.30.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "rollup": "^3.25.3",
    "rome": "^12.1.3",
    "svelte-check": "^3.4.4",
    "svelte-preprocess": "^5.0.4",
    "typescript": "5.1.3",
    "vite": "4.3.9",
    "vitest": "0.32.2"
  },
  "dependencies": {
    "svelte": "^4.0.0",
    "ts-clone-node": "^2.0.4"
  },
  "files": ["lib", "src"]
}
